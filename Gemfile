
source "https://rubygems.org"

gem "jekyll"

# Jekyll will automatically activate any plugins
# listed in the :jekyll_plugins group.
group :jekyll_plugins do
  gem 'jekyll-redirect-from'
  gem "jekyll-compose"
  gem "jekyll-seo-tag"
  gem "jekyll-paginate"
  gem "jekyll-feed"
  gem "jekyll-sitemap"
  gem "jekyll-github-metadata"
end

group :development do
	gem "rake"
  gem "webrick"
end